<launch>
  <arg
    name="model" />
  <arg
    name="gui"
    default="False" />
  <param
    name="robot_description"
    textfile="$(find minesweeper_fixed)/urdf/minesweeper_fixed.urdf" />
  <param
    name="use_gui"
    value="true" />
  <node
    name="joint_state_publisher"
    pkg="joint_state_publisher"
    type="joint_state_publisher" />
  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="state_publisher" />
  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find minesweeper_fixed)/urdf.rviz" />
  <node pkg="tf" 
   type="static_transform_publisher" 
   name="link1_broadcaster" 
   args="5 -4.5 0 0 0 0.5 0.5 world test_link 100" />
<arg name="initial_pose_x" value="5.0"/> <arg name="initial_pose_y" value="-4.5"/> <arg name="initial_pose_z" value="0.0"/>
</launch>
